{
  "name": "texlab/html",
  "description": "Set of classes for view templates",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "TexLab\\Html\\": "src"
    }
  },
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
